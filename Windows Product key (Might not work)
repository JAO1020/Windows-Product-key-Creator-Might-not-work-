import random
import string

def activate_windows_xp():
    key_sections = [''.join(random.choices(string.ascii_uppercase + string.digits, k=5)) for _ in range(5)]
    license_key = '-'.join(key_sections)
    return license_key

if __name__ == "__main__":
    generated_key = activate_windows_xp()
    print(generated_key)
